#!/bin/bash

rm -rf build

rm *.iml *.ipr *.iws
rm */*.iml

rm -rf .gradle .idea .vscode .mypy_cache .pytest_cache .cache */.pytest_cache */.mypy_cache

find . -type f -name '*.pyc' -delete
find . -type d -name __pycache__ | xargs rm -rf
find . -type d -name mainGeneratedDataTemplate | xargs rm -rf
find . -type d -name mainGeneratedRest | xargs rm -rf
find . -type d -name testGeneratedDataTemplate | xargs rm -rf
