#!/usr/bin/env fish

# Install packages via Cargo.

set -l PACKAGES amber \
    autorebase \
    cargo-binstall \
    cargo-cache \
    cargo-edit \
    cargo-nextest \
    cargo-quickinstall \
    cargo-update \
    csvlens \
    dirstat-rs \
    gfold \
    jfmt \
    lfs \
    lolcate-rs \
    ppcp \
    rnr \
    rustcat

command cargo install $PACKAGES
