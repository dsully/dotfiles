{
  "neodev": {
    "library": {
      "enabled": true,
      "runtime": true,
      "types": true
    }
  },
  "neoconf": {
    "plugins": {
      "lspconfig": {
        "enabled": true
      },
      "lua_ls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua": {
        "completion": {
          "autoRequire": false,
          "callSnippet": "Replace",
          "keywordSnippet": "Both",
          "workspaceWord": true
        },
        "diagnostics": {
          "disable": ["missing-fields"],
          "globals": ["bit", "describe", "it", "vim"],
          "unusedLocalExclude": ["_*"]
        },
        "doc": {
          "privateName": ["^_"]
        },
        "format": {
          "enable": false
        },
        "hint": {
          "arrayIndex": "Disable",
          "enable": true,
          "paramName": "Disable",
          "paramType": true,
          "semicolon": "Disable",
          "setType": true
        },
        "runtime": {
          "version": "LuaJIT"
        },
        "telemetry": {
          "enable": false
        },
        "type": {
          "castNumberToInteger": true
        },
        "workspace": {
          "checkThirdParty": false
        }
      }
    }
  }
}
